{
  "name": "swaggerjsontoapidocs",
  "version": "1.7.0",
  "description": "script to convert swagger json to api docs, this help us to consume functions and center all endpoints in one place",
  "keywords": [
    "swagger",
    "swagger-json",
    "api-docs",
    "api-documentation",
    "cli",
    "typescript",
    "nodejs",
    "swagger-to-docs",
    "swagger-to-api",
    "json-to-api",
    "api-generator"
  ],
  "author": "Esteban Fernandez <efernandezdev@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/efernandezdev/swaggerjsontoapidocs.git"
  },
  "homepage": "https://github.com/efernandezdev/swaggerjsontoapidocs#readme",
  "publishConfig": {
    "access": "public"
  },
  "main": "bin/index.js",
  "scripts": {
    "build": "tsc",
    "api": "ts-node ./src/index.ts",
    "dist": "node ./bin/index.js",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "swaggerjsontoapidocs": "bin/index.js"
  },
  "dependencies": {
    "chalk": "5.6.2",
    "fs-extra": "11.3.3",
    "prettier": "3.7.4",
    "yargs": "18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.2",
    "@types/fs-extra": "11.0.4",
    "@types/prettier": "2.7.3",
    "@types/yargs": "17.0.35",
    "eslint": "9.39.2",
    "globals": "17.0.0",
    "jiti": "2.6.1",
    "semantic-release": "^25.0.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.52.0"
  },
  "files": [
    "bin",
    "package.json",
    "!bin/api_docs"
  ],
  "engines": {
    "node": ">=20.19.0"
  }
}
